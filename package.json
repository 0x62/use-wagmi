{
  "name": "use-wagmi",
  "version": "0.0.1",
  "packageManager": "pnpm@7.6.0",
  "description": "Vue Composition for Ethereum",
  "keywords": [
    "vue",
    "hooks",
    "eth",
    "ethereum",
    "dapps",
    "wallet",
    "web3",
    "wagmi",
    "use-wagmi"
  ],
  "homepage": "https://github.com/unicape/use-wagmi#readme",
  "bugs": {
    "url": "https://github.com/unicape/use-wagmi/issues"
  },
  "license": "MIT",
  "author": "Unic Ape <94770222@qq.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/unicape/use-wagmi.git"
  },
  "scripts": {
    "dev": "pnpm --filter playground dev",
    "build": "esno scripts/build.ts",
    "build:rollup": "cross-env NODE_OPTIONS=\"--max-old-space-size=6144\" rollup -c",
    "publish:ci": "esno scripts/publish.ts",
    "clean": "rimraf dist",
    "watch": "esno scripts/build.ts --watch"
  },
  "main": "./index.cjs",
  "module": "./index.mjs",
  "types": "./index.d.ts",
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "require": "./index.cjs",
      "import": "./index.mjs"
    },
    "./core": {
      "types": "./core/index.d.ts",
      "require": "./core/index.cjs",
      "import": "./core/index.mjs"
    },
    "./chains": {
      "types": "./chains/index.d.ts",
      "require": "./chains/index.cjs",
      "import": "./chains/index.mjs"
    },
    "./actions": {
      "types": "./actions/index.d.ts",
      "require": "./actions/index.cjs",
      "import": "./actions/index.mjs"
    },
    "./providers": {
      "types": "./providers/index.d.ts",
      "require": "./providers/index.cjs",
      "import": "./providers/index.mjs"
    },
    "./connectors": {
      "types": "./connectors/index.d.ts",
      "require": "./connectors/index.cjs",
      "import": "./connectors/index.mjs"
    }
  },
  "dependencies": {
    "@wagmi/core": "^0.10.6",
    "abitype": "^0.3.0",
    "vue-demi": "latest",
    "vue-query": "^2.0.0-beta.12"
  },
  "devDependencies": {
    "@antfu/ni": "^0.20.0",
    "@types/fs-extra": "^11.0.1",
    "@types/node": "^18.14.0",
    "consola": "^2.15.3",
    "cross-env": "^7.0.3",
    "ethers": "^5.7.1",
    "esbuild-register": "^3.4.2",
    "esno": "^0.16.3",
    "fs-extra": "^11.1.0",
    "rimraf": "^4.1.2",
    "rollup": "^3.17.2",
    "rollup-plugin-dts": "^5.2.0",
    "rollup-plugin-esbuild": "^5.0.0",
    "typescript": "^4.9.5",
    "vue": "^3.2.47"
  },
  "peerDependencies": {
    "@vue/composition-api": "^1.0.0-rc.1",
    "ethers": ">=5.5.1 <6",
    "vue": "^2.0.0 || >=3.0.0"
  },
  "peerDependenciesMeta": {
    "@vue/composition-api": {
      "optional": true
    }
  }
}
